
<div class="nested-child" id=<%= element.js_id %> data-toggle="close" data-type="child">
  <span class="nested-caret"><%= link_to "&#8710;", element, remote: true %></span>
  <span><%= link_to element.bank, element %></span>
  <span><%= element.periodicity %></span>
  <span><%= element.quantity %> Cartolas - <%= link_to element.society, society_path(element.society) %></span>
  <span class="options-right"><%= element.progress scope %> %</span>
  <span class="options-right"><%= link_to "Tiempos (#{element.time_nodes(scope).size})", times_tax_path(element), remote: true %></span>
  <span class="options-right"><%= link_to "Cartolas (#{element.filter(scope).size})", element, remote: true %></span>
</div>